{
  "version": 2,
  "buildCommand": "npx vite build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/debug-speaker-issue",
      "destination": "/debug-speaker-issue.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}