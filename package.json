{
  "name": "apax-knowledge-now-2025",
  "version": "1.0.0",
  "description": "Apax KnowledgeNow 2025 - Conference PWA Application",
  "type": "module",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "python3 -m http.server 3000",
    "serve": "python3 -m http.server 3000",
    "server": "node server-with-auth.js",
    "test": "NODE_OPTIONS='--max-old-space-size=4096' vitest",
    "test:watch": "NODE_OPTIONS='--max-old-space-size=4096' vitest --watch",
    "test:coverage": "NODE_OPTIONS='--max-old-space-size=4096' vitest --coverage",
    "test:ci": "NODE_OPTIONS='--max-old-space-size=6144' vitest run --coverage",
    "test:batch": "node test-runner.js",
    "test:quick": "NODE_OPTIONS='--max-old-space-size=4096' vitest run --reporter=basic --bail=5",
    "test:ui": "vitest --ui",
    "test-connection": "node test-direct-connection.js",
    "show-counts": "node show-table-counts.js",
    "spike": "node scripts/spike-server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niozzo/KN-React-App.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/niozzo/KN-React-App/issues"
  },
  "homepage": "https://github.com/niozzo/KN-React-App#readme",
  "dependencies": {
    "@supabase/supabase-js": "^2.57.2",
    "@types/react-router-dom": "^5.3.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "pg": "^8.11.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "react-transition-group": "^4.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "c8": "^10.1.3",
    "jsdom": "^26.1.0",
    "msw": "^2.11.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^3.2.4",
    "workbox-window": "^7.3.0"
  }
}
